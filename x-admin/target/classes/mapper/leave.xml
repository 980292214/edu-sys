<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.mapper.leaveMapper">
    <select id="findByTeacherId"  resultType="java.util.Map">
     SELECT * FROM t_leave WHERE teacherid=#{teacherId}
    </select>

    <select id="findByStudentId"  resultType="java.util.Map">
     SELECT * FROM t_leave WHERE studentid=#{studentId}
    </select>
</mapper>
