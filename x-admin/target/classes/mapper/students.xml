<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.mapper.studentsMapper">


    <select resultType="com.example.entity.students" id="getStudentid">
        select * from t_students where studentid = #{username}
    </select>

    <select resultType="com.example.entity.students" id="getStu">
        select * from t_students where studentid = #{studentid}
    </select>
</mapper>
